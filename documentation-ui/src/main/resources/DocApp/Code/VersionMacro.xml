    <property>
      <code>{{velocity}}
#set ($discard = $xwiki.ssx.use($wikimacro.doc.documentReference))
#set ($versions = [])
#set ($plainVersions = [])
#set ($product = $wikimacro.parameters.product)
#if ("$!wikimacro.parameters.since" != '')
  #foreach ($item in $stringtool.split($wikimacro.parameters.since, ','))
    ## Don't display RC text, always final version since users don't really care about RCs
    ## But we still want exact version metadata in the usage of the macro
    #set ($item = $stringtool.substringBefore($item, "RC"))
    #set ($discard = $versions.add("${stringtool.trim($item)}+"))
    #set ($discard = $plainVersions.add("${stringtool.trim($item)}"))
  #end
  #set ($dataSuffix = 'since')
#else
  #foreach ($item in $stringtool.split($wikimacro.parameters.before, ','))
    ## Don't display RC text, always final version since users don't really care about RCs
    ## But we still want exact version metadata in the usage of the macro
    #set ($item = $stringtool.substringBefore($item, "RC"))
    #set ($discard = $versions.add("&lt;${stringtool.trim($item)}"))
    #set ($discard = $plainVersions.add("${stringtool.trim($item)}"))
  #end
  #set ($dataSuffix = 'before')
#end
#set ($xdom = $services.rendering.parse($xcontext.macro.content, $wikimacro.context.getSyntax().toIdString()))
## If the macro's content starts with a heading, the children are just a single section block, so check the actual type.
## Same for a single list or a single table.
## In practice, only a single paragraph is truly supported as inline content in XWiki.
#if ($xdom.children.size() &gt; 1 || ($xdom.children.size() == 1 &amp;&amp; !$objecttool.instanceOf($xdom.children.get(0), 'org.xwiki.rendering.block.ParagraphBlock')))
  #set ($severalBlocksAttribute = " data-xwikiorg-versions-range='$xdom.children.size()'")
#else
  #set ($severalBlocksAttribute = '')
#end
## Treat a single paragraph as inline content to display the badge on the same line.
#if ($wikimacro.context.isInline() || $severalBlocksAttribute == '')
(% class='badge' title="$stringtool.capitalize($dataSuffix) $product $stringtool.join($plainVersions, ', ')" data-xwikiorg-product="$product" data-xwikiorg-versions-${dataSuffix}="$stringtool.join($plainVersions, ', ')"${severalBlocksAttribute} %)$product $stringtool.join($versions, ', ')(% data-xwikiorg-product="$product" data-xwikiorg-versions-${dataSuffix}="$stringtool.join($plainVersions, ', ')"${severalBlocksAttribute} %) {{wikimacrocontent/}}
#else
(% data-xwikiorg-product="$product" data-xwikiorg-versions-${dataSuffix}="$stringtool.join($plainVersions, ', ')"${severalBlocksAttribute} %)(((
(% class='badge' title="$stringtool.capitalize($dataSuffix) $product $stringtool.join($plainVersions, ', ')" %)$product $stringtool.join($versions, ', ')(%%)

{{wikimacrocontent/}}
)))
#end
{{/velocity}}</code>
